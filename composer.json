{
  "name": "darkanakin41/api-bundle",
  "type": "symfony-bundle",
  "description": "Pierre LEJEUNE ApiBundle",
  "keywords": [
    "api",
    "symfony",
    "bundle"
  ],
  "homepage": "https://github.com/darkanakin41/api-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Pierre LEJEUNE",
      "email": "lejeune.pierre.41@gmail.com"
    }
  ],
  "require": {
    "php": "^7",
    "ext-curl": "*",
    "darkanakin41/core-bundle": "dev-master"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Darkanakin41\\ApiBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Darkanakin41\\ApiBundle\\Tests\\": "tests/"
    }
  }
}
