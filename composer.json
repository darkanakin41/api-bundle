{
  "name": "plejeune/api-bundle",
  "type": "symfony-bundle",
  "description": "Pierre LEJEUNE ApiBundle",
  "keywords": [
    "Media Bundle"
  ],
  "homepage": "https://gitlab.com/pierrelejeune/api-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Pierre LEJEUNE",
      "email": "lejeune.pierre.41@gmail.com"
    }
  ],
  "require": {
    "php": "^7.*",
    "ext-curl": "*",
    "knplabs/knp-paginator-bundle": "dev-master",
    "plejeune/core-bundle": "dev-master",
    "symfony/serializer-pack": "dev-master",
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0"
    }
  },
  "autoload": {
    "psr-4": {
      "PLejeune\\ApiBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  }
}